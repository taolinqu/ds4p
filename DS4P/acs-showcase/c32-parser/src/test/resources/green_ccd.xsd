	<xs:complexType name="pivlTs">
		<xs:sequence>
			<xs:element name="period">
				<xs:complexType>
					<xs:attribute name="value"/>
					<xs:attribute name="unit"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="institutionSpecified" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="cs">
		<xs:attribute name="code"/>
	</xs:complexType>
	<xs:element name="qualifier">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name">
					<xs:complexType>
						<xs:complexContent>
							<xs:restriction base="cd">
								<xs:sequence>
									<xs:element name="originalText" type="xs:string" minOccurs="0"/>
									<xs:element ref="qualifier" minOccurs="0" maxOccurs="0"/>
								</xs:sequence>
							</xs:restriction>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="value">
					<xs:complexType>
						<xs:complexContent>
							<xs:restriction base="cd">
								<xs:sequence>
									<xs:element name="originalText" type="xs:string" minOccurs="0"/>
									<xs:element ref="qualifier" minOccurs="0" maxOccurs="0"/>
								</xs:sequence>
							</xs:restriction>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
