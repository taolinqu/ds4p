							</xs:sequence>
						</xs:restriction>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="freeTextProductName" type="xs:string"/>
			<xs:element name="freeTextBrandName" type="xs:string" minOccurs="0"/>
			<xs:element name="drugManufacturer" type="organizationInformation" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="providerInformation">
		<xs:annotation>
			<xs:documentation>Reusable provider information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="providerType" type="cd" minOccurs="0"/>
			<xs:element name="providerID" type="ii" minOccurs="0"/>
			<xs:element name="providerAddress" type="addr" minOccurs="0"/>
			<xs:element name="providerTelecom" type="tele" minOccurs="0"/>
			<xs:element name="providerName" type="pnm" minOccurs="0"/>
