		<xs:sequence>
			<xs:element name="organizationID" type="ii" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="organizationAddress" type="addr" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="organizationTelecom" type="tele" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="organizationName" type="onm" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="provider" type="organizationInformation"/>
	<xs:element name="informationSource">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="author">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="authorTime" type="ts"/>
							<xs:element name="authorName" type="pnm"/>
							<xs:element name="reference" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="referenceDocumentID" type="ii"/>
										<xs:element name="referenceDocumentURL">
											<xs:complexType>
												<xs:attribute name="value"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="informationSourceName" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:element name="personName" type="pnm"/>
							<xs:element name="organizationName" type="onm"/>
							<xs:element name="relatedPersonName" type="pnm"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="supportType">
