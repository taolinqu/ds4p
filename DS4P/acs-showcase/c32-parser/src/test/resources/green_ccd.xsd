												<xs:sequence>
												<xs:element name="treatingProviderID" type="ii"
												minOccurs="0"/>
												</xs:sequence>
												</xs:complexType>
												</xs:element>
												<xs:element name="ageAtOnset" type="xs:integer"
												minOccurs="0"/>
												<xs:element name="causeOfDeath" minOccurs="0">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="problemCode" type="cd"
												minOccurs="0"/>
												<xs:element name="timeOfDeath" type="ts"
												minOccurs="0"/>
												<xs:element name="ageAtDeath" type="xs:integer"
												minOccurs="0"/>
												</xs:sequence>
												</xs:complexType>
												</xs:element>
												<xs:element name="problemStatus" type="ce"
												minOccurs="0"/>
												<xs:element ref="comment" minOccurs="0"
												maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="planOfCare" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="text" type="textType" minOccurs="0"/>
										<xs:choice maxOccurs="unbounded">
											<xs:element name="plannedObservation"
												type="plannedEvent"/>
											<xs:element name="plannedProcedure" type="plannedEvent"/>
											<xs:element name="plannedAct" type="plannedEvent"/>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="socialHistory" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="text" type="textType" minOccurs="0"/>
										<xs:element name="socialHistoryEntry"
											type="socialHistoryEntry" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>

						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="results">
		<xs:annotation>
			<xs:documentation>Check with Bob Dolin. Do we need to handle results organizers? C83
				does not mention them. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="text" type="textType" minOccurs="0"/>
				<xs:element name="result" type="result" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ii">
		<xs:annotation>
			<xs:documentation>Id</xs:documentation>
		</xs:annotation>
		<xs:attribute name="root"/>
		<xs:attribute name="extension"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="cd">
		<xs:annotation>
			<xs:documentation>Code</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="originalText" minOccurs="0"/>
			<xs:element ref="qualifier" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="code"/>
		<xs:attribute name="displayName"/>
		<xs:attribute name="codeSystem"/>
		<xs:attribute name="codeSystemName"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="pq">
		<xs:annotation>
			<xs:documentation>Physical Quantity</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value"/>
		<xs:attribute name="unit"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="mo">
		<xs:annotation>
			<xs:documentation>Monetary Amount</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required"/>
		<xs:attribute name="currency" type="currencyType" use="optional"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="onm" mixed="true">
		<xs:annotation>
			<xs:documentation>Organization name</xs:documentation>
		</xs:annotation>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="pnm">
		<xs:annotation>
			<xs:documentation>Person Name</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="prefix" minOccurs="0"/>
			<xs:element name="given"/>
			<xs:element name="family"/>
			<xs:element name="suffix" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="addr">
		<xs:annotation>
			<xs:documentation>Address (US only)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="streetAddressLine" minOccurs="0" maxOccurs="2"/>
			<xs:element name="city" minOccurs="0"/>
			<xs:element name="state" minOccurs="0"/>
			<xs:element name="postalCode" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="tele">
		<xs:annotation>
			<xs:documentation>Telecom</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="ts">
		<xs:attribute name="value"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
	</xs:complexType>
	<xs:complexType name="ivlTs">
		<xs:choice minOccurs="0">
			<xs:sequence>
				<xs:element name="low" type="ts">
					<xs:annotation>
						<xs:documentation> The low limit of the interval. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice minOccurs="0">
					<xs:element name="width" type="pq" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The difference between high and low boundary. The
								purpose of distinguishing a width property is to handle all cases of
								incomplete information symmetrically. In any interval representation
								only two of the three properties high, low, and width need to be
								stated and the third can be derived. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="high" type="ts" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The high limit of the interval. </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:element name="high" type="ts">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:sequence>
				<xs:element name="width" type="pq">
					<xs:annotation>
						<xs:documentation> The difference between high and low boundary. The purpose
							of distinguishing a width property is to handle all cases of incomplete
							information symmetrically. In any interval representation only two of
							the three properties high, low, and width need to be stated and the
							third can be derived. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="high" type="ts" minOccurs="0">
					<xs:annotation>
						<xs:documentation> The high limit of the interval. </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="center" type="ts">
					<xs:annotation>
						<xs:documentation> The arithmetic mean of the interval (low plus high
							divided by 2). The purpose of distinguishing the center as a semantic
							property is for conversions of intervals from and to point values.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="width" type="pq" minOccurs="0">
					<xs:annotation>
						<xs:documentation> The difference between high and low boundary. The purpose
							of distinguishing a width property is to handle all cases of incomplete
							information symmetrically. In any interval representation only two of
							the three properties high, low, and width need to be stated and the
							third can be derived. </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="value"/>
		<xs:attribute name="nullFlavor" type="nullFlavorType"/>
		<xs:attribute name="operator"/>
	</xs:complexType>
	<xs:complexType name="value">
		<xs:sequence>
			<xs:choice>
				<xs:element name="integer">
					<xs:complexType>
						<xs:attribute name="value"/>
						<xs:attribute name="nullFlavor" type="nullFlavorType"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="monetaryAmount" type="mo"/>
				<xs:element name="physicalQuantity" type="pq"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="result">
		<xs:annotation>
			<xs:documentation>Reusable result</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="resultID" type="ii" maxOccurs="unbounded"/>
			<xs:element name="refusalReason" type="cd"
				minOccurs="0"/>
			<!-- required -->
			<xs:element name="resultDateTime" type="ivlTs"/>
			<xs:element name="procedureCode" type="cd"/>
			<!-- required -->
			<xs:element name="resultType" type="cd"/>
			<!-- required -->
			<xs:element name="resultStatus" type="cs"/>
			<!-- required -->
			<!-- optional -->
			<xs:element name="resultValue" type="value" minOccurs="0"/>
			<xs:element name="resultInterpretation" type="cd" minOccurs="0"/>
			<!-- optional -->
			<xs:element name="resultReferenceRange" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Skip reference range, review with Bob Dolin to see what is
						important to capture. </xs:documentation>
				</xs:annotation>
				<!-- optional -->
			</xs:element>
			<xs:element ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="medicationInformation">
		<xs:annotation>
			<xs:documentation>Reusable medical information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="codedProductName">
				<xs:complexType>
