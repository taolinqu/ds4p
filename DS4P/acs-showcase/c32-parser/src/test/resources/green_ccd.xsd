			<xs:element name="socialHistoryId" type="ii"/>
			<xs:element name="socialHistoryTime" type="ivlTs" minOccurs="0"/>
			<xs:element name="socialHistoryType" type="cd"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
